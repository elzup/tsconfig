{
	"compilerOptions": {
		"module": "esnext",
		"moduleResolution": "nodenext",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"noImplicitAny": false,
		"jsx": "preserve",

		"target": "esnext",
		"lib": ["es7", "dom"],
		"sourceMap": true
	}
}
